
<h1><%=wt '%{page_title} - Comparing versions %{old_version} and %{new_version}', :page_title => h( @draft.name ), :old_version => @old_version.number, :new_version => @new_version.number %></h1>

<%= link_to "Back to history", draft_history_path(@draft) %><br />

<%=wt 'Changes list:' %>

<%= render partial:'draft_page_history', locals: { draft: @draft, versions: @versions, with_form: false} %>


<%=wt 'Changes in content:' %>

<div class="wiki_diff">
  <%= draft_diff @old_version.content, @new_version.content %>
</div>

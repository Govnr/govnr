<% breadcrumb :user, @user %>

<%= render partial: "users/header" %>
 
<div class="container-fluid timeline-container">
<div class="timeline">
  <div class="line"></div>
        <%= render partial: 'groups/activity/activity', collection: @activities, locals: {small:false}, cached: true %>
  </div>
</div>

<% if @user.id == current_user.id %>
<%= link_to 'Edit', edit_user_path(@user) %> |
<% end %>
<%= link_to 'Back', users_path %>